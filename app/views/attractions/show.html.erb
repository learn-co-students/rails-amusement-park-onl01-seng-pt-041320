<%= @attraction.name %>
<% if @user.admin == false %>
<%= button_to "Go on this ride", {:controller => "users", :action => "update", :attraction_id => @attraction.id, :user_id=> session[:user_id]} , :method=>:post  %>
<% else %>
<%= link_to "Edit Attraction", edit_attraction_path(@attraction) %>
<% end %>

